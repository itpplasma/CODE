#%Module

module-whatis "Sets paths you need to use FreeFEM, version 4.2.1"

proc ModulesHelp { } {
puts stderr "This is the Modulefile for configuring the environment to use"
}

proc ModulesDisplay {} {
}

conflict freefem

setenv FREEFEM_HOME /temp/ert/FreeFEM/4.2.1

prepend-path PATH        $env(FREEFEM_HOME)/bin
prepend-path LD_LIBRARY_PATH $env(FREEFEM_HOME)/lib

